{
  "name": "@synerise/ds-information-card",
  "version": "0.6.1",
  "description": "InformationCard UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "lint": "eslint . --ext .ts --ext .tsx --max-warnings 0 --ignore-pattern dist",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^0.8.37",
    "@synerise/ds-avatar": "^0.17.20",
    "@synerise/ds-badge": "^0.8.18",
    "@synerise/ds-button": "^0.21.18",
    "@synerise/ds-card": "^0.19.15",
    "@synerise/ds-description": "^0.4.1",
    "@synerise/ds-divider": "^0.7.14",
    "@synerise/ds-dropdown": "^0.18.23",
    "@synerise/ds-icon": "^0.66.4",
    "@synerise/ds-menu": "^0.20.5",
    "@synerise/ds-scrollbar": "^0.11.18",
    "@synerise/ds-subtle-form": "^0.9.135",
    "@synerise/ds-tooltip": "^0.14.48",
    "@synerise/ds-typography": "^0.16.5",
    "@synerise/ds-utils": "^0.31.1",
    "rc-trigger": "5.2.5",
    "react-intl": "6.8.4"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 <= 17.0.2",
    "styled-components": "5.0.1"
  }
}
