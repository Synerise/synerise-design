{
  "name": "@synerise/ds-information-card",
  "version": "0.4.4",
  "description": "InformationCard UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "lint": "eslint . --ext .ts --ext .tsx --max-warnings 0 --ignore-pattern dist",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^0.8.13",
    "@synerise/ds-avatar": "^0.16.0",
    "@synerise/ds-badge": "^0.7.0",
    "@synerise/ds-button": "^0.20.0",
    "@synerise/ds-card": "^0.16.17",
    "@synerise/ds-description": "^0.3.105",
    "@synerise/ds-divider": "^0.6.19",
    "@synerise/ds-dropdown": "^0.17.110",
    "@synerise/ds-icon": "^0.61.0",
    "@synerise/ds-menu": "^0.18.23",
    "@synerise/ds-subtle-form": "^0.9.109",
    "@synerise/ds-tooltip": "^0.14.28",
    "@synerise/ds-typography": "^0.15.0",
    "rc-trigger": "5.2.5"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@synerise/ds-utils": "^0.26.6",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1"
  }
}
