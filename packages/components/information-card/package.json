{
  "name": "@synerise/ds-information-card",
  "version": "0.3.39",
  "description": "InformationCard UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "lint": "eslint . --ext .ts --ext .tsx --max-warnings 0 --ignore-pattern dist",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^0.7.44",
    "@synerise/ds-avatar": "^0.15.6",
    "@synerise/ds-badge": "^0.6.38",
    "@synerise/ds-button": "^0.18.3",
    "@synerise/ds-card": "^0.15.5",
    "@synerise/ds-description": "^0.3.82",
    "@synerise/ds-divider": "^0.5.44",
    "@synerise/ds-dropdown": "^0.17.81",
    "@synerise/ds-icon": "^0.58.3",
    "@synerise/ds-menu": "^0.17.20",
    "@synerise/ds-subtle-form": "^0.9.76",
    "@synerise/ds-tooltip": "^0.14.5",
    "@synerise/ds-typography": "^0.13.1",
    "rc-trigger": "5.2.5"
  },
  "peerDependencies": {
    "@synerise/ds-core": "^0.31.2",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@synerise/ds-utils": "^0.24.17",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1"
  }
}
