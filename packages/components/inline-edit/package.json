{
  "name": "@synerise/ds-inline-edit",
  "version": "0.6.84",
  "description": "InlineEdit UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-dropdown": "^0.17.84",
    "@synerise/ds-icon": "^0.58.5",
    "@synerise/ds-menu": "^0.18.2",
    "@synerise/ds-scrollbar": "^0.6.10",
    "@synerise/ds-search": "^0.8.69",
    "@synerise/ds-tooltip": "^0.14.7",
    "@synerise/ds-typography": "^0.13.2",
    "react-input-autosize": "^2.2.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@synerise/ds-utils": "^0.24.19",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "@types/react-input-autosize": "2.2.1"
  }
}
