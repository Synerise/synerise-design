{
  "name": "@synerise/ds-format-picker",
  "version": "0.7.9",
  "description": "FormatPicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../storybook/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.23.4",
    "@synerise/ds-button-group": "^0.8.4",
    "@synerise/ds-checkbox": "^0.13.7",
    "@synerise/ds-data-format": "^0.7.2",
    "@synerise/ds-dropdown": "^0.19.9",
    "@synerise/ds-icon": "^0.72.1",
    "@synerise/ds-list-item": "^0.7.7",
    "@synerise/ds-radio": "^0.13.7",
    "@synerise/ds-tooltip": "^0.16.3",
    "@synerise/ds-typography": "^0.17.7",
    "@synerise/ds-utils": "^0.32.3"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
