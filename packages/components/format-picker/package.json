{
  "name": "@synerise/ds-format-picker",
  "version": "0.5.13",
  "description": "FormatPicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.21.15",
    "@synerise/ds-button-group": "^0.7.19",
    "@synerise/ds-checkbox": "^0.12.13",
    "@synerise/ds-data-format": "^0.5.4",
    "@synerise/ds-dropdown": "^0.18.20",
    "@synerise/ds-icon": "^0.66.1",
    "@synerise/ds-list-item": "^0.4.15",
    "@synerise/ds-radio": "^0.12.9",
    "@synerise/ds-tooltip": "^0.14.45",
    "@synerise/ds-typography": "^0.16.2",
    "@synerise/ds-utils": "^0.30.1",
    "react-intl": "3.12.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 <= 17.0.2",
    "styled-components": "5.0.1"
  }
}
