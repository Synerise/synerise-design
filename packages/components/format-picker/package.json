{
  "name": "@synerise/ds-format-picker",
  "version": "0.4.4",
  "description": "FormatPicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.21.4",
    "@synerise/ds-button-group": "^0.7.7",
    "@synerise/ds-checkbox": "^0.12.3",
    "@synerise/ds-dropdown": "^0.18.7",
    "@synerise/ds-icon": "^0.64.1",
    "@synerise/ds-list-item": "^0.4.3",
    "@synerise/ds-radio": "^0.11.75",
    "@synerise/ds-tooltip": "^0.14.34",
    "@synerise/ds-typography": "^0.15.1",
    "@types/numeral": "^2.0.2",
    "lodash": "^4.17.19",
    "numeral": "^2.0.6",
    "react-intl": "3.12.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 <= 17.0.2",
    "styled-components": "5.0.1"
  }
}
