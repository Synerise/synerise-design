{
  "name": "@synerise/ds-format-picker",
  "version": "0.3.57",
  "description": "FormatPicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.17.12",
    "@synerise/ds-button-group": "^0.6.19",
    "@synerise/ds-checkbox": "^0.11.34",
    "@synerise/ds-dropdown": "^0.17.61",
    "@synerise/ds-icon": "^0.54.0",
    "@synerise/ds-menu": "^0.17.5",
    "@synerise/ds-radio": "^0.11.34",
    "@types/numeral": "^0.0.28",
    "numeral": "^2.0.6"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
