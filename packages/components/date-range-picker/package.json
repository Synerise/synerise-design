{
  "name": "@synerise/ds-date-range-picker",
  "version": "0.1.26",
  "description": "Date-Range-Picker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@date-fns/upgrade": "^1.0.3",
    "@synerise/ds-button": "^0.13.1",
    "@synerise/ds-date-picker": "^0.2.4",
    "@synerise/ds-icon": "^0.31.0",
    "@synerise/ds-time-picker": "^0.3.16",
    "@synerise/ds-tooltip": "^0.8.2",
    "date-fns": "^2.16.1",
    "lodash": "^4.17.20",
    "ramda": "^0.27.0",
    "react-day-picker": "^7.4.8"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "@types/ramda": "^0.27.14"
  }
}
