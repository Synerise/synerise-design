{
  "name": "@synerise/ds-date-range-picker",
  "version": "1.5.1",
  "description": "Date-Range-Picker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run build:js && pnpm run build:css && pnpm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "pnpm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "pnpm pack --pack-destination ../../storybook/storybook-static/static",
    "prepublish": "pnpm run build",
    "test": "vitest",
    "test:watch": "pnpm run test -- --watch",
    "types": "tsc --noEmit",
    "check:circular-dependencies": "node -e 'const pkg = require(\"./package.json\").name; console.log(\"\\x1b[31m%s\\x1b[0m\", `[WARN] skipping circular dependencies in ${pkg}`)'",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@date-fns/upgrade": "^1.0.3",
    "@synerise/ds-badge": "workspace:^",
    "@synerise/ds-button": "workspace:^",
    "@synerise/ds-button-group": "workspace:^",
    "@synerise/ds-date-picker": "workspace:^",
    "@synerise/ds-dropdown": "workspace:^",
    "@synerise/ds-form-field": "workspace:^",
    "@synerise/ds-icon": "workspace:^",
    "@synerise/ds-inline-edit": "workspace:^",
    "@synerise/ds-input": "workspace:^",
    "@synerise/ds-input-number": "workspace:^",
    "@synerise/ds-list-item": "workspace:^",
    "@synerise/ds-manageable-list": "workspace:^",
    "@synerise/ds-scrollbar": "workspace:^",
    "@synerise/ds-select": "workspace:^",
    "@synerise/ds-slider": "workspace:^",
    "@synerise/ds-tag": "workspace:^",
    "@synerise/ds-time-picker": "workspace:^",
    "@synerise/ds-tooltip": "workspace:^",
    "@synerise/ds-utils": "workspace:^",
    "date-fns": "^2.16.1",
    "date-fns-tz": "1.1.4",
    "dayjs": "^1.8.35",
    "lodash": "^4.17.21",
    "ramda": "^0.27.0",
    "react-day-picker": "^7.4.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/user-event": "14",
    "@types/lodash": "^4.17.20",
    "vitest": "4"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.24.16",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
