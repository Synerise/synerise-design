{
  "name": "@synerise/ds-date-range-picker",
  "version": "0.32.0",
  "description": "Date-Range-Picker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest && yarn test:timezones",
    "test:timezone-ny": "TZ=America/New_York jest -t TimezoneTesting",
    "test:timezone-utc": "TZ=UTC jest -t TimezoneTesting",
    "test:timezone-waw": "TZ=Europe/Warsaw jest -t TimezoneTesting",
    "test:timezones": "yarn test:timezone-utc && yarn test:timezone-ny && yarn test:timezone-waw",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@date-fns/upgrade": "^1.0.3",
    "@synerise/ds-badge": "^0.9.0",
    "@synerise/ds-button": "^0.23.0",
    "@synerise/ds-button-group": "^0.8.0",
    "@synerise/ds-data-format": "^0.7.0",
    "@synerise/ds-date-picker": "^0.14.0",
    "@synerise/ds-dropdown": "^0.19.0",
    "@synerise/ds-icon": "^0.71.0",
    "@synerise/ds-inline-edit": "^0.10.0",
    "@synerise/ds-input": "^0.25.0",
    "@synerise/ds-input-number": "^0.11.0",
    "@synerise/ds-list-item": "^0.7.0",
    "@synerise/ds-manageable-list": "^0.30.0",
    "@synerise/ds-scrollbar": "^0.12.0",
    "@synerise/ds-select": "^0.17.0",
    "@synerise/ds-slider": "^0.15.0",
    "@synerise/ds-tags": "^0.11.0",
    "@synerise/ds-time-picker": "^0.12.0",
    "@synerise/ds-tooltip": "^0.15.0",
    "@synerise/ds-utils": "^0.32.0",
    "date-fns": "^2.16.1",
    "date-fns-tz": "1.1.4",
    "dayjs": "^1.8.35",
    "lodash": "^4.17.21",
    "ramda": "^0.27.0",
    "react-day-picker": "^7.4.8",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.24.16",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
