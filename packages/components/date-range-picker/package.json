{
  "name": "@synerise/ds-date-range-picker",
  "version": "1.4.2",
  "description": "Date-Range-Picker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../storybook/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "check:circular-dependencies": "node -e 'const pkg = require(\"./package.json\").name; console.log(\"\\x1b[31m%s\\x1b[0m\", `[WARN] skipping circular dependencies in ${pkg}`)'",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@date-fns/upgrade": "^1.0.3",
    "@synerise/ds-badge": "^1.0.26",
    "@synerise/ds-button": "^1.5.1",
    "@synerise/ds-button-group": "^1.1.25",
    "@synerise/ds-date-picker": "^1.3.2",
    "@synerise/ds-dropdown": "^1.1.2",
    "@synerise/ds-form-field": "^1.1.20",
    "@synerise/ds-icon": "^1.9.0",
    "@synerise/ds-inline-edit": "^1.1.2",
    "@synerise/ds-input": "^1.5.1",
    "@synerise/ds-input-number": "^1.2.16",
    "@synerise/ds-list-item": "^1.2.1",
    "@synerise/ds-manageable-list": "^1.6.2",
    "@synerise/ds-scrollbar": "^1.2.1",
    "@synerise/ds-select": "^1.3.1",
    "@synerise/ds-slider": "^1.0.27",
    "@synerise/ds-tag": "^1.4.1",
    "@synerise/ds-time-picker": "^1.2.2",
    "@synerise/ds-tooltip": "^1.3.1",
    "@synerise/ds-utils": "^1.5.0",
    "date-fns": "^2.16.1",
    "date-fns-tz": "1.1.4",
    "dayjs": "^1.8.35",
    "lodash": "^4.17.21",
    "ramda": "^0.27.0",
    "react-day-picker": "^7.4.8",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.24.16",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
