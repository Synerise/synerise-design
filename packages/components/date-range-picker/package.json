{
  "name": "@synerise/ds-date-range-picker",
  "version": "0.23.1",
  "description": "Date-Range-Picker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@date-fns/upgrade": "^1.0.3",
    "@synerise/ds-badge": "^0.6.33",
    "@synerise/ds-button": "^0.18.0",
    "@synerise/ds-button-group": "^0.6.28",
    "@synerise/ds-data-format": "^0.4.1",
    "@synerise/ds-date-picker": "^0.10.11",
    "@synerise/ds-dropdown": "^0.17.74",
    "@synerise/ds-icon": "^0.58.0",
    "@synerise/ds-inline-edit": "^0.6.73",
    "@synerise/ds-input": "^0.18.48",
    "@synerise/ds-input-number": "^0.8.21",
    "@synerise/ds-manageable-list": "^0.26.2",
    "@synerise/ds-menu": "^0.17.16",
    "@synerise/ds-scrollbar": "^0.6.5",
    "@synerise/ds-slider": "^0.12.23",
    "@synerise/ds-tags": "^0.8.8",
    "@synerise/ds-time-picker": "^0.8.2",
    "@synerise/ds-tooltip": "^0.14.2",
    "@synerise/ds-utils": "^0.19.0",
    "date-fns": "^2.16.1",
    "dayjs": "^1.8.35",
    "lodash": "^4.17.20",
    "ramda": "^0.27.0",
    "react-day-picker": "^7.4.8",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "^4",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@types/ramda": "^0.27.14"
  }
}
