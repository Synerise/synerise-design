{
  "name": "@synerise/ds-tags",
  "version": "0.2.50",
  "description": "Tags UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.9.10",
    "@synerise/ds-dropdown": "^0.11.17",
    "@synerise/ds-icon": "^0.14.3",
    "@synerise/ds-search-bar": "^0.2.23"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "@synerise/ds-utils": "^0.8.14"
  }
}
