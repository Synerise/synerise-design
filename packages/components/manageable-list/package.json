{
  "name": "@synerise/ds-manageable-list",
  "version": "0.24.6",
  "description": "ManageableList UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-avatar": "^0.14.55",
    "@synerise/ds-button": "^0.17.12",
    "@synerise/ds-dropdown": "^0.17.54",
    "@synerise/ds-icon": "^0.54.0",
    "@synerise/ds-inline-edit": "^0.6.51",
    "@synerise/ds-input": "^0.18.35",
    "@synerise/ds-list": "^0.6.29",
    "@synerise/ds-menu": "^0.17.4",
    "@synerise/ds-modal": "^0.16.10",
    "@synerise/ds-popconfirm": "^0.10.3",
    "@synerise/ds-result": "^0.6.19",
    "@synerise/ds-tags": "^0.7.11",
    "@synerise/ds-tooltip": "^0.12.1",
    "@synerise/ds-typography": "^0.12.7",
    "@synerise/ds-utils": "^0.19.0",
    "lodash": "^4.17.19",
    "react-animate-height": "^2.0.23",
    "react-sortablejs": "^6.0.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "@synerise/ds-inline-edit": "^0.0.15",
    "antd": "^4",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "^4.4.0"
  }
}
