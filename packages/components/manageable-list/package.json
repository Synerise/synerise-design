{
  "name": "@synerise/ds-manageable-list",
  "version": "0.26.30",
  "description": "ManageableList UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-avatar": "^0.15.24",
    "@synerise/ds-button": "^0.19.3",
    "@synerise/ds-dropdown": "^0.17.99",
    "@synerise/ds-icon": "^0.60.3",
    "@synerise/ds-inline-edit": "^0.6.99",
    "@synerise/ds-input": "^0.20.0",
    "@synerise/ds-list": "^0.6.61",
    "@synerise/ds-menu": "^0.18.15",
    "@synerise/ds-modal": "^0.17.21",
    "@synerise/ds-popconfirm": "^0.10.28",
    "@synerise/ds-result": "^0.6.47",
    "@synerise/ds-tags": "^0.8.33",
    "@synerise/ds-tooltip": "^0.14.20",
    "@synerise/ds-typography": "^0.14.3",
    "@synerise/ds-utils": "^0.25.0",
    "lodash": "^4.17.19",
    "moment": "^2.25.3",
    "react-animate-height": "^2.0.23",
    "react-intl": "3.12.0",
    "react-sortablejs": "^6.0.0",
    "sortablejs": "1.15.1"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.7.0",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@testing-library/react": "10.0.1"
  }
}
