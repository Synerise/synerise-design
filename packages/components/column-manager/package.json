{
  "name": "@synerise/ds-column-manager",
  "version": "0.13.8",
  "description": "ColumnManager UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^0.8.43",
    "@synerise/ds-button": "^0.22.0",
    "@synerise/ds-drawer": "^0.6.31",
    "@synerise/ds-dropdown": "^0.18.28",
    "@synerise/ds-icon": "^0.68.0",
    "@synerise/ds-input": "^0.24.18",
    "@synerise/ds-input-number": "^0.10.19",
    "@synerise/ds-item-filter": "^0.9.7",
    "@synerise/ds-menu": "^0.20.9",
    "@synerise/ds-modal": "^0.17.54",
    "@synerise/ds-result": "^0.7.17",
    "@synerise/ds-scrollbar": "^0.11.21",
    "@synerise/ds-search-bar": "^0.7.6",
    "@synerise/ds-select": "^0.16.30",
    "@synerise/ds-switch": "^0.6.28",
    "@synerise/ds-tooltip": "^0.14.52",
    "@synerise/ds-typography": "^0.16.9",
    "@synerise/ds-utils": "^0.31.2",
    "react-sortablejs": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.7.0",
    "react": ">=16.9.0 <= 17.0.2",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "5.0.1"
  }
}
