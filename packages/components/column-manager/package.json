{
  "name": "@synerise/ds-column-manager",
  "version": "0.12.8",
  "description": "ColumnManager UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^0.8.34",
    "@synerise/ds-button": "^0.21.15",
    "@synerise/ds-drawer": "^0.6.27",
    "@synerise/ds-dropdown": "^0.18.20",
    "@synerise/ds-icon": "^0.66.1",
    "@synerise/ds-input": "^0.24.10",
    "@synerise/ds-input-number": "^0.10.11",
    "@synerise/ds-item-filter": "^0.8.156",
    "@synerise/ds-menu": "^0.20.2",
    "@synerise/ds-modal": "^0.17.47",
    "@synerise/ds-result": "^0.7.10",
    "@synerise/ds-scrollbar": "^0.11.15",
    "@synerise/ds-search-bar": "^0.6.95",
    "@synerise/ds-select": "^0.16.22",
    "@synerise/ds-switch": "^0.6.20",
    "@synerise/ds-tooltip": "^0.14.45",
    "@synerise/ds-typography": "^0.16.2",
    "@synerise/ds-utils": "^0.30.1",
    "react-intl": "3.12.0",
    "react-sortablejs": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.7.0",
    "react": ">=16.9.0 <= 17.0.2",
    "styled-components": "5.0.1"
  }
}
