{
  "name": "@synerise/ds-column-manager",
  "version": "1.0.9",
  "description": "ColumnManager UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../storybook/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^1.1.0",
    "@synerise/ds-button": "^1.2.0",
    "@synerise/ds-drawer": "^1.0.2",
    "@synerise/ds-dropdown": "^1.0.7",
    "@synerise/ds-icon": "^1.4.2",
    "@synerise/ds-input": "^1.1.2",
    "@synerise/ds-input-number": "^1.1.2",
    "@synerise/ds-item-filter": "^1.0.9",
    "@synerise/ds-menu": "^1.0.7",
    "@synerise/ds-modal": "^1.0.7",
    "@synerise/ds-result": "^1.0.7",
    "@synerise/ds-scrollbar": "^1.0.7",
    "@synerise/ds-search-bar": "^1.1.2",
    "@synerise/ds-select": "^1.1.2",
    "@synerise/ds-switch": "^1.1.2",
    "@synerise/ds-tooltip": "^1.1.6",
    "@synerise/ds-typography": "^1.0.7",
    "@synerise/ds-utils": "^1.1.0",
    "react-sortablejs": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.24.16",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
