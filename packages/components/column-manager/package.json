{
  "name": "@synerise/ds-column-manager",
  "version": "0.10.53",
  "description": "ColumnManager UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.17.11",
    "@synerise/ds-drawer": "^0.5.28",
    "@synerise/ds-dropdown": "^0.17.52",
    "@synerise/ds-icon": "^0.53.0",
    "@synerise/ds-input": "^0.18.33",
    "@synerise/ds-item-filter": "^0.8.53",
    "@synerise/ds-result": "^0.6.18",
    "@synerise/ds-scrollbar": "^0.5.4",
    "@synerise/ds-search-bar": "^0.6.23",
    "@synerise/ds-tooltip": "^0.12.0",
    "@synerise/ds-typography": "^0.12.7",
    "@synerise/ds-utils": "^0.19.0",
    "react-final-form": "^6.3.5",
    "react-sortablejs": "^6.0.0",
    "uuid": "^8.1.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "moment": "^2.24.0"
  }
}
