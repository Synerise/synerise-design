{
  "name": "@synerise/ds-context-selector",
  "version": "0.25.23",
  "description": "ContextSelector UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "gitHead": "fb9af91dbebaf4d5ac135be15a78712786cca0f2",
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.22.1",
    "@synerise/ds-divider": "^0.7.19",
    "@synerise/ds-dropdown": "^0.18.29",
    "@synerise/ds-factors": "^0.27.10",
    "@synerise/ds-icon": "^0.69.0",
    "@synerise/ds-information-card": "^0.7.5",
    "@synerise/ds-list-item": "^0.6.1",
    "@synerise/ds-menu": "^0.20.10",
    "@synerise/ds-result": "^0.7.18",
    "@synerise/ds-scrollbar": "^0.11.22",
    "@synerise/ds-skeleton": "^0.6.22",
    "@synerise/ds-tabs": "^0.17.12",
    "@synerise/ds-utils": "^0.31.2",
    "react-window": "1.8.5",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 <= 17.0.2",
    "styled-components": "^5.0.1"
  }
}
