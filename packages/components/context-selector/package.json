{
  "name": "@synerise/ds-context-selector",
  "version": "0.21.6",
  "description": "ContextSelector UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "gitHead": "fb9af91dbebaf4d5ac135be15a78712786cca0f2",
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.19.6",
    "@synerise/ds-dropdown": "^0.17.103",
    "@synerise/ds-factors": "^0.22.6",
    "@synerise/ds-icon": "^0.60.6",
    "@synerise/ds-information-card": "^0.3.66",
    "@synerise/ds-menu": "^0.18.18",
    "@synerise/ds-result": "^0.6.50",
    "@synerise/ds-scrollbar": "^0.10.1",
    "@synerise/ds-skeleton": "^0.5.4",
    "@synerise/ds-tabs": "^0.13.103",
    "@synerise/ds-utils": "^0.26.2",
    "react-window": "1.8.5",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/user-event": "^10.3.1"
  }
}
