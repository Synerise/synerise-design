{
  "name": "@synerise/ds-factors",
  "version": "1.9.0",
  "description": "Factors UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "pnpm pack --pack-destination ../../storybook/storybook-static/static",
    "prepublish": "npm run build",
    "test": "vitest",
    "test:watch": "npm run test -- --watch",
    "types": "tsc --noEmit",
    "check:circular-dependencies": "madge --circular --extensions ts,tsx,js,jsx --ts-config tsconfig.json src/ --exclude '/dist/'",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "workspace:^",
    "@synerise/ds-autocomplete": "workspace:^",
    "@synerise/ds-badge": "workspace:^",
    "@synerise/ds-button": "workspace:^",
    "@synerise/ds-button-group": "workspace:^",
    "@synerise/ds-collector": "workspace:^",
    "@synerise/ds-date-picker": "workspace:^",
    "@synerise/ds-date-range-picker": "workspace:^",
    "@synerise/ds-divider": "workspace:^",
    "@synerise/ds-dropdown": "workspace:^",
    "@synerise/ds-empty-states": "workspace:^",
    "@synerise/ds-icon": "workspace:^",
    "@synerise/ds-information-card": "workspace:^",
    "@synerise/ds-inline-edit": "workspace:^",
    "@synerise/ds-input": "workspace:^",
    "@synerise/ds-input-number": "workspace:^",
    "@synerise/ds-list-item": "workspace:^",
    "@synerise/ds-modal": "workspace:^",
    "@synerise/ds-result": "workspace:^",
    "@synerise/ds-scrollbar": "workspace:^",
    "@synerise/ds-search": "workspace:^",
    "@synerise/ds-select": "workspace:^",
    "@synerise/ds-short-cuts": "workspace:^",
    "@synerise/ds-skeleton": "workspace:^",
    "@synerise/ds-tabs": "workspace:^",
    "@synerise/ds-tag": "workspace:^",
    "@synerise/ds-tooltip": "workspace:^",
    "@synerise/ds-utils": "workspace:^",
    "lodash": "^4.17.21",
    "react-window": "^1.8.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.24.16",
    "copy-to-clipboard": "^3.3.1",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3",
    "vitest": "4"
  }
}
