{
  "name": "@synerise/ds-factors",
  "version": "0.19.2",
  "description": "Factors UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-autocomplete": "^0.4.47",
    "@synerise/ds-badge": "^0.6.33",
    "@synerise/ds-button": "^0.18.0",
    "@synerise/ds-date-picker": "^0.10.11",
    "@synerise/ds-date-range-picker": "^0.23.1",
    "@synerise/ds-dropdown": "^0.17.74",
    "@synerise/ds-icon": "^0.58.0",
    "@synerise/ds-information-card": "^0.3.31",
    "@synerise/ds-inline-edit": "^0.6.73",
    "@synerise/ds-input": "^0.18.48",
    "@synerise/ds-input-number": "^0.8.21",
    "@synerise/ds-loader": "^0.2.41",
    "@synerise/ds-menu": "^0.17.16",
    "@synerise/ds-modal": "^0.17.2",
    "@synerise/ds-result": "^0.6.28",
    "@synerise/ds-scrollbar": "^0.6.5",
    "@synerise/ds-tabs": "^0.13.71",
    "@synerise/ds-tooltip": "^0.14.2",
    "@synerise/ds-typography": "^0.12.8",
    "@synerise/ds-utils": "^0.24.13",
    "classnames": "^2.2.6",
    "react-window": "1.8.5"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
