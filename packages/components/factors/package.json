{
  "name": "@synerise/ds-factors",
  "version": "1.6.0",
  "description": "Factors UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../storybook/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "check:circular-dependencies": "madge --circular --extensions ts,tsx,js,jsx --ts-config tsconfig.json src/ --exclude '/dist/'",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^1.1.25",
    "@synerise/ds-autocomplete": "^1.2.15",
    "@synerise/ds-badge": "^1.0.25",
    "@synerise/ds-button": "^1.5.0",
    "@synerise/ds-button-group": "^1.1.24",
    "@synerise/ds-collector": "^1.4.4",
    "@synerise/ds-date-picker": "^1.3.0",
    "@synerise/ds-date-range-picker": "^1.4.0",
    "@synerise/ds-divider": "^1.2.1",
    "@synerise/ds-dropdown": "^1.1.0",
    "@synerise/ds-empty-states": "^1.0.21",
    "@synerise/ds-icon": "^1.8.0",
    "@synerise/ds-information-card": "^1.3.0",
    "@synerise/ds-inline-edit": "^1.1.0",
    "@synerise/ds-input": "^1.5.0",
    "@synerise/ds-input-number": "^1.2.15",
    "@synerise/ds-list-item": "^1.2.0",
    "@synerise/ds-menu": "^1.2.0",
    "@synerise/ds-modal": "^1.2.17",
    "@synerise/ds-result": "^1.0.30",
    "@synerise/ds-scrollbar": "^1.2.0",
    "@synerise/ds-search": "^1.3.10",
    "@synerise/ds-select": "^1.3.0",
    "@synerise/ds-skeleton": "^1.0.25",
    "@synerise/ds-tabs": "^1.1.0",
    "@synerise/ds-tag": "^1.4.0",
    "@synerise/ds-tooltip": "^1.3.0",
    "@synerise/ds-utils": "^1.5.0",
    "copy-to-clipboard": "^3.3.3",
    "lodash": "^4.17.21",
    "react-window": "^1.8.11",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.24.16",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
