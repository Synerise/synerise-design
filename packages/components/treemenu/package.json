{
  "name": "@synerise/ds-treemenu",
  "version": "0.2.7",
  "description": "Treemenu UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-dropdown": "^0.17.5",
    "@synerise/ds-icon": "^0.46.2",
    "@synerise/ds-input": "^0.18.3",
    "@synerise/ds-menu": "^0.11.4",
    "@synerise/ds-result": "^0.6.3",
    "@synerise/ds-search": "^0.6.8",
    "@synerise/ds-utils": "^0.19.0",
    "array-move": "^3.0.1",
    "random-hash": "^4.0.1",
    "react-sortable-hoc": "^1.11.0",
    "react-sortablejs": "^6.0.0",
    "simplebar-react": "^2.3.3",
    "tree-model": "^1.0.7"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "@synerise/ds-utils": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
