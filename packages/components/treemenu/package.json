{
  "name": "@synerise/ds-treemenu",
  "version": "0.5.7",
  "description": "Treemenu UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.22.0",
    "@synerise/ds-button-group": "^0.7.26",
    "@synerise/ds-dropdown": "^0.18.27",
    "@synerise/ds-icon": "^0.68.0",
    "@synerise/ds-input": "^0.24.17",
    "@synerise/ds-loader": "^0.3.22",
    "@synerise/ds-menu": "^0.20.9",
    "@synerise/ds-popconfirm": "^0.10.64",
    "@synerise/ds-result": "^0.7.17",
    "@synerise/ds-scrollbar": "^0.11.21",
    "@synerise/ds-search": "^0.9.22",
    "@synerise/ds-search-bar": "^0.7.5",
    "@synerise/ds-tooltip": "^0.14.52",
    "@synerise/ds-typography": "^0.16.9",
    "@synerise/ds-utils": "^0.31.2",
    "array-move": "^3.0.1",
    "random-hash": "^4.0.1",
    "rc-menu": "8.10.8",
    "react-sortable-hoc": "^1.11.0",
    "simplebar-react": "^2.3.3",
    "tree-model": "^1.0.7",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 <= 17.0.2",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "5.0.1"
  }
}
