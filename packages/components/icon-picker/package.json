{
  "name": "@synerise/ds-icon-picker",
  "version": "0.3.87",
  "description": "IconPicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.18.7",
    "@synerise/ds-dropdown": "^0.17.87",
    "@synerise/ds-icon": "^0.58.7",
    "@synerise/ds-scrollbar": "^0.6.12",
    "@synerise/ds-utils": "^0.24.21"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1"
  }
}
