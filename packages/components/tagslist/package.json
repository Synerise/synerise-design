{
  "name": "@synerise/ds-tagslist",
  "version": "0.9.9",
  "description": "TagsList UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.18.2",
    "@synerise/ds-checkbox": "^0.11.44",
    "@synerise/ds-checkbox-tristate": "^0.2.44",
    "@synerise/ds-dropdown": "^0.17.80",
    "@synerise/ds-icon": "^0.58.2",
    "@synerise/ds-input": "^0.19.2",
    "@synerise/ds-loader": "^0.2.44",
    "@synerise/ds-menu": "^0.17.19",
    "@synerise/ds-result": "^0.6.31",
    "@synerise/ds-scrollbar": "^0.6.7",
    "@synerise/ds-search": "^0.8.65",
    "@synerise/ds-search-bar": "^0.6.44",
    "@synerise/ds-tooltip": "^0.14.4",
    "@synerise/ds-utils": "^0.24.16",
    "@types/react-highlight-words": "^0.16.1",
    "invariant": "2.2.4",
    "random-hash": "^4.0.1",
    "react-highlight-words": "^0.17.0",
    "react-intl": "3.12.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.7.0",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1"
  }
}
