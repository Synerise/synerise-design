{
  "name": "@synerise/ds-tagslist",
  "version": "0.8.0",
  "description": "TagsList UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.16.3",
    "@synerise/ds-checkbox-tristate": "^0.2.5",
    "@synerise/ds-dropdown": "^0.17.6",
    "@synerise/ds-icon": "^0.46.2",
    "@synerise/ds-menu": "^0.12.0",
    "@synerise/ds-search": "^0.7.0",
    "@synerise/ds-utils": "^0.19.0",
    "@types/react-highlight-words": "^0.16.1",
    "random-hash": "^4.0.1",
    "react-highlight-words": "^0.17.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
