{
  "name": "@synerise/ds-tagslist",
  "version": "0.10.5",
  "description": "TagsList UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.21.5",
    "@synerise/ds-checkbox": "^0.12.4",
    "@synerise/ds-checkbox-tristate": "^0.2.71",
    "@synerise/ds-dropdown": "^0.18.9",
    "@synerise/ds-icon": "^0.65.0",
    "@synerise/ds-input": "^0.23.6",
    "@synerise/ds-list-item": "^0.4.5",
    "@synerise/ds-loader": "^0.3.10",
    "@synerise/ds-result": "^0.6.62",
    "@synerise/ds-scrollbar": "^0.11.5",
    "@synerise/ds-search": "^0.9.5",
    "@synerise/ds-search-bar": "^0.6.84",
    "@synerise/ds-tooltip": "^0.14.35",
    "@synerise/ds-typography": "^0.15.1",
    "@synerise/ds-utils": "^0.28.2",
    "invariant": "2.2.4",
    "react-highlight-words": "^0.17.0",
    "react-intl": "3.12.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.7.0",
    "react": ">=16.9.0 <= 17.0.2",
    "styled-components": "^5.0.1"
  }
}
