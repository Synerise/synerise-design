{
  "name": "@synerise/ds-tagslist",
  "version": "0.11.2",
  "description": "TagsList UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.21.18",
    "@synerise/ds-checkbox": "^0.12.16",
    "@synerise/ds-checkbox-tristate": "^0.2.84",
    "@synerise/ds-dropdown": "^0.18.23",
    "@synerise/ds-icon": "^0.66.4",
    "@synerise/ds-input": "^0.24.13",
    "@synerise/ds-list-item": "^0.4.18",
    "@synerise/ds-loader": "^0.3.21",
    "@synerise/ds-result": "^0.7.13",
    "@synerise/ds-scrollbar": "^0.11.18",
    "@synerise/ds-search": "^0.9.18",
    "@synerise/ds-search-bar": "^0.7.1",
    "@synerise/ds-tooltip": "^0.14.48",
    "@synerise/ds-typography": "^0.16.5",
    "@synerise/ds-utils": "^0.31.1",
    "invariant": "2.2.4",
    "react-highlight-words": "^0.17.0",
    "react-intl": "6.8.4",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.7.0",
    "react": ">=16.9.0 <= 17.0.2",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@types/invariant": "^2.2.37"
  }
}
