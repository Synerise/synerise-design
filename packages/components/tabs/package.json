{
  "name": "@synerise/ds-tabs",
  "version": "0.13.79",
  "description": "Tabs UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.18.3",
    "@synerise/ds-dropdown": "^0.17.81",
    "@synerise/ds-icon": "^0.58.3",
    "@synerise/ds-menu": "^0.17.20",
    "@synerise/ds-typography": "^0.13.1",
    "@synerise/ds-utils": "^0.24.17",
    "classnames": "2.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.7.0",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1"
  }
}
