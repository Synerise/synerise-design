{
  "name": "@synerise/ds-time-picker",
  "version": "0.11.4",
  "description": "TimePicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx' --ignore '**/*.spec.ts' --ignore '**/*.spec.tsx' --ignore '**/*.spec.constants.ts' --ignore '**/*.spec.constants.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest && yarn test:timezones",
    "test:timezone-ny": "TZ=America/New_York jest -t TimezoneTesting",
    "test:timezone-utc": "TZ=UTC jest -t TimezoneTesting",
    "test:timezone-waw": "TZ=Europe/Warsaw jest -t TimezoneTesting",
    "test:timezones": "yarn test:timezone-utc && yarn test:timezone-ny && yarn test:timezone-waw",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-data-format": "^0.6.1",
    "@synerise/ds-dropdown": "^0.18.26",
    "@synerise/ds-icon": "^0.68.0",
    "@synerise/ds-input": "^0.24.16",
    "@synerise/ds-scrollbar": "^0.11.21",
    "@synerise/ds-tooltip": "^0.14.51",
    "@synerise/ds-typography": "^0.16.8",
    "@synerise/ds-utils": "^0.31.2",
    "dayjs": "^1.8.19",
    "debounce": "^1.2.0",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.24.0",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
