{
  "name": "@synerise/ds-time-picker",
  "version": "0.8.0",
  "description": "TimePicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx' --ignore '**/*.spec.ts' --ignore '**/*.spec.tsx' --ignore '**/*.spec.constants.ts' --ignore '**/*.spec.constants.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-data-format": "^0.4.0",
    "@synerise/ds-dropdown": "^0.17.72",
    "@synerise/ds-icon": "^0.58.0",
    "@synerise/ds-input": "^0.18.46",
    "@synerise/ds-scrollbar": "^0.6.4",
    "@synerise/ds-search-bar": "^0.6.36",
    "@synerise/ds-typography": "^0.12.8",
    "@synerise/ds-utils": "^0.24.12",
    "dayjs": "^1.8.19",
    "debounce": "^1.2.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "@types/debounce": "^1.2.0"
  }
}
