{
  "name": "@synerise/ds-list-item",
  "version": "0.6.3",
  "description": "ListItem UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-icon": "^0.70.0",
    "@synerise/ds-tooltip": "^0.14.55",
    "@synerise/ds-utils": "^0.31.2",
    "copy-to-clipboard": "^3.3.1",
    "rc-trigger": "^5.2.5",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.7.0",
    "react": ">=16.9.0 <= 17.0.2",
    "styled-components": "5.0.1"
  }
}
