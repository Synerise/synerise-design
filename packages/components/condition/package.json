{
  "name": "@synerise/ds-condition",
  "version": "0.21.6",
  "description": "Condition UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.19.0",
    "@synerise/ds-context-selector": "^0.20.3",
    "@synerise/ds-core": "^0.40.10",
    "@synerise/ds-cruds": "^0.4.3",
    "@synerise/ds-factors": "^0.21.3",
    "@synerise/ds-icon": "^0.60.0",
    "@synerise/ds-inline-edit": "^0.6.95",
    "@synerise/ds-operators": "^0.10.54",
    "@synerise/ds-subject": "^0.9.82",
    "@synerise/ds-tooltip": "^0.14.16",
    "@synerise/ds-utils": "^0.24.24",
    "lodash": "^4.17.19",
    "react-intl": "3.12.0",
    "react-sortablejs": "^6.0.0",
    "sortablejs": "1.15.1"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "uuid": "^8.3.2"
  }
}
