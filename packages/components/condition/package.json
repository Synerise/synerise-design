{
  "name": "@synerise/ds-condition",
  "version": "0.1.1",
  "description": "Condition UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.12.33",
    "@synerise/ds-core": "*",
    "@synerise/ds-factors": "^0.1.1",
    "@synerise/ds-inline-edit": "^0.1.63",
    "@synerise/ds-operators": "^0.1.1",
    "@synerise/ds-subject": "^0.1.0",
    "@synerise/ds-tooltip": "^0.7.17",
    "@synerise/ds-utils": "^0.10.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
