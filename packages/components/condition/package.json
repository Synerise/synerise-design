{
  "name": "@synerise/ds-condition",
  "version": "0.27.13",
  "description": "Condition UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../storybook/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.23.3",
    "@synerise/ds-context-selector": "^0.26.13",
    "@synerise/ds-cruds": "^0.6.6",
    "@synerise/ds-factors": "^0.28.13",
    "@synerise/ds-icon": "^0.72.0",
    "@synerise/ds-inline-edit": "^0.10.10",
    "@synerise/ds-input": "^0.25.8",
    "@synerise/ds-operators": "^0.15.10",
    "@synerise/ds-subject": "^0.11.8",
    "@synerise/ds-tooltip": "^0.16.2",
    "@synerise/ds-utils": "^0.32.2",
    "lodash": "^4.17.19",
    "react-sortablejs": "^6.0.0",
    "sortablejs": "1.15.1"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
