{
  "name": "@synerise/ds-sidebar",
  "version": "0.2.6",
  "description": "Sidebar UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-block": "^0.3.6",
    "@synerise/ds-button": "^0.9.6",
    "@synerise/ds-card": "^0.4.6",
    "@synerise/ds-icon": "^0.14.2",
    "@synerise/ds-typography": "^0.8.2",
    "immutability-helper": "^3.0.1",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "^4",
    "react": ">=16.9.0 < 17.0.0"
  }
}
