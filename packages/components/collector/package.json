{
  "name": "@synerise/ds-collector",
  "version": "0.10.4",
  "description": "Collector UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.21.1",
    "@synerise/ds-divider": "^0.7.0",
    "@synerise/ds-icon": "^0.63.0",
    "@synerise/ds-input": "^0.23.0",
    "@synerise/ds-list-item": "^0.3.0",
    "@synerise/ds-menu": "^0.19.3",
    "@synerise/ds-scrollbar": "^0.11.2",
    "@synerise/ds-search": "^0.8.95",
    "@synerise/ds-utils": "^0.27.0",
    "classnames": "2.3.2",
    "react-intl": "^3.12.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.7.0",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "^11"
  }
}
