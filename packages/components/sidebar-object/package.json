{
  "name": "@synerise/ds-sidebar-object",
  "version": "0.7.9",
  "description": "SidebarObject UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.23.2",
    "@synerise/ds-description": "^0.5.5",
    "@synerise/ds-drawer": "^0.7.2",
    "@synerise/ds-dropdown": "^0.19.7",
    "@synerise/ds-icon": "^0.71.2",
    "@synerise/ds-inline-edit": "^0.10.9",
    "@synerise/ds-menu": "^0.21.8",
    "@synerise/ds-result": "^0.8.6",
    "@synerise/ds-scrollbar": "^0.12.5",
    "@synerise/ds-subtle-form": "^0.10.7",
    "@synerise/ds-tabs": "^0.18.9",
    "@synerise/ds-typography": "^0.17.5",
    "@synerise/ds-utils": "^0.32.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.24.16",
    "react": ">=16.9.0 <= 18.3.1",
    "styled-components": "^5.3.3"
  }
}
