{
  "name": "@synerise/ds-item-picker",
  "version": "0.13.0",
  "description": "ItemPicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-avatar": "^0.17.19",
    "@synerise/ds-button": "^0.21.17",
    "@synerise/ds-dropdown": "^0.18.22",
    "@synerise/ds-flag": "^0.5.3",
    "@synerise/ds-icon": "^0.66.3",
    "@synerise/ds-input": "^0.24.12",
    "@synerise/ds-list-item": "^0.4.17",
    "@synerise/ds-popconfirm": "^0.10.58",
    "@synerise/ds-result": "^0.7.12",
    "@synerise/ds-scrollbar": "^0.11.17",
    "@synerise/ds-search": "^0.9.17",
    "@synerise/ds-search-bar": "^0.7.0",
    "@synerise/ds-tooltip": "^0.14.47",
    "@synerise/ds-typography": "^0.16.4",
    "@synerise/ds-utils": "^0.31.0",
    "react-intl": "6.8.4"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 <= 17.0.2",
    "styled-components": "5.0.1"
  }
}
