{
  "name": "@synerise/ds-item-picker",
  "version": "0.11.16",
  "description": "ItemPicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.18.0",
    "@synerise/ds-dropdown": "^0.17.75",
    "@synerise/ds-icon": "^0.58.0",
    "@synerise/ds-input": "^0.18.49",
    "@synerise/ds-menu": "^0.17.16",
    "@synerise/ds-popconfirm": "^0.10.13",
    "@synerise/ds-result": "^0.6.28",
    "@synerise/ds-scrollbar": "^0.6.5",
    "@synerise/ds-search": "^0.8.62",
    "@synerise/ds-search-bar": "^0.6.39",
    "@synerise/ds-tooltip": "^0.14.2",
    "@synerise/ds-typography": "^0.12.8",
    "react-intl": "3.12.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@synerise/ds-avatar": "^0.15.0",
    "@synerise/ds-flag": "^0.4.0",
    "@synerise/ds-utils": "^0.24.14",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1"
  }
}
