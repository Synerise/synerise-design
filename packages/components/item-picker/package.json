{
  "name": "@synerise/ds-item-picker",
  "version": "0.15.1",
  "description": "ItemPicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../storybook/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-avatar": "^0.18.0",
    "@synerise/ds-button": "^0.23.0",
    "@synerise/ds-dropdown": "^0.19.0",
    "@synerise/ds-flag": "^0.6.0",
    "@synerise/ds-icon": "^0.71.0",
    "@synerise/ds-information-card": "^0.8.1",
    "@synerise/ds-input": "^0.25.0",
    "@synerise/ds-list-item": "^0.7.0",
    "@synerise/ds-popconfirm": "^0.11.0",
    "@synerise/ds-result": "^0.8.0",
    "@synerise/ds-scrollbar": "^0.12.0",
    "@synerise/ds-search": "^0.10.1",
    "@synerise/ds-search-bar": "^0.8.0",
    "@synerise/ds-tooltip": "^0.15.0",
    "@synerise/ds-typography": "^0.17.0",
    "@synerise/ds-utils": "^0.32.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
