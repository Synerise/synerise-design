{
  "name": "@synerise/ds-item-picker",
  "version": "0.11.25",
  "description": "ItemPicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.18.4",
    "@synerise/ds-dropdown": "^0.17.83",
    "@synerise/ds-icon": "^0.58.4",
    "@synerise/ds-input": "^0.19.4",
    "@synerise/ds-menu": "^0.18.1",
    "@synerise/ds-popconfirm": "^0.10.17",
    "@synerise/ds-result": "^0.6.33",
    "@synerise/ds-scrollbar": "^0.6.9",
    "@synerise/ds-search": "^0.8.68",
    "@synerise/ds-search-bar": "^0.6.46",
    "@synerise/ds-tooltip": "^0.14.6",
    "@synerise/ds-typography": "^0.13.2",
    "react-intl": "3.12.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@synerise/ds-avatar": "^0.15.8",
    "@synerise/ds-flag": "^0.4.2",
    "@synerise/ds-utils": "^0.24.18",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1"
  }
}
