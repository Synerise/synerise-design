{
  "name": "@synerise/ds-item-picker",
  "version": "1.12.0",
  "description": "ItemPicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../storybook/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "check:circular-dependencies": "madge --circular ./src --extensions ts,tsx,js,jsx --ts-config tsconfig.json --exclude '/dist/'",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^1.1.25",
    "@synerise/ds-avatar": "^1.1.0",
    "@synerise/ds-button": "^1.5.0",
    "@synerise/ds-dropdown": "^1.1.0",
    "@synerise/ds-empty-states": "^1.0.21",
    "@synerise/ds-flag": "^1.0.5",
    "@synerise/ds-form-field": "^1.1.19",
    "@synerise/ds-icon": "^1.8.0",
    "@synerise/ds-information-card": "^1.3.0",
    "@synerise/ds-list-item": "^1.2.0",
    "@synerise/ds-loader": "^1.0.10",
    "@synerise/ds-popconfirm": "^1.0.29",
    "@synerise/ds-result": "^1.0.30",
    "@synerise/ds-scrollbar": "^1.2.0",
    "@synerise/ds-search": "^1.3.10",
    "@synerise/ds-search-bar": "^1.4.0",
    "@synerise/ds-short-cuts": "^1.0.23",
    "@synerise/ds-skeleton": "^1.0.25",
    "@synerise/ds-tooltip": "^1.3.0",
    "@synerise/ds-utils": "^1.5.0",
    "lodash": "^4.17.21",
    "react-window": "^1.8.11",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
