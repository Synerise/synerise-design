{
  "name": "@synerise/ds-search",
  "version": "0.1.14",
  "description": "Search UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.12.7",
    "@synerise/ds-dropdown": "^0.11.37",
    "@synerise/ds-scrollbar": "^0.1.0",
    "@synerise/ds-typography": "^0.8.3",
    "@synerise/ds-utils": "^0.8.22",
    "@types/react-virtualized": "^9.21.9",
    "react-onclickoutside": "^6.9.0",
    "react-virtualized": "^9.21.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "@types/react-onclickoutside": "^6.7.3"
  }
}
