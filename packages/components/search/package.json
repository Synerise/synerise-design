{
  "name": "@synerise/ds-search",
  "version": "0.8.8",
  "description": "Search UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.17.3",
    "@synerise/ds-dropdown": "^0.17.23",
    "@synerise/ds-icon": "^0.49.1",
    "@synerise/ds-menu": "^0.15.2",
    "@synerise/ds-scrollbar": "^0.4.8",
    "@synerise/ds-typography": "^0.12.3",
    "@synerise/ds-utils": "^0.19.0",
    "react-onclickoutside": "^6.9.0",
    "react-window": "^1.8.5"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "@types/react-onclickoutside": "^6.7.3",
    "@types/react-window": "^1.8.2"
  }
}
