{
  "name": "@synerise/ds-input",
  "version": "0.16.3",
  "description": "Input UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-icon": "^0.34.0",
    "@synerise/ds-scrollbar": "^0.2.35",
    "@synerise/ds-tooltip": "^0.9.3",
    "@synerise/ds-typography": "^0.9.5",
    "@synerise/ds-utils": "^0.11.7",
    "antd-mask-input": "^0.1.13",
    "uuid": "^7.0.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "^4",
    "react": ">=16.9.0 < 17.0.0"
  }
}
