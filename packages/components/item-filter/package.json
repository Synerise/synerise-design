{
  "name": "@synerise/ds-item-filter",
  "version": "0.8.30",
  "description": "ItemFilter UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.17.9",
    "@synerise/ds-drawer": "^0.5.21",
    "@synerise/ds-dropdown": "^0.17.33",
    "@synerise/ds-icon": "^0.51.0",
    "@synerise/ds-manageable-list": "^0.23.30",
    "@synerise/ds-result": "^0.6.16",
    "@synerise/ds-scrollbar": "^0.5.2",
    "@synerise/ds-search-bar": "^0.6.11",
    "@synerise/ds-tabs": "^0.13.28",
    "@synerise/ds-typography": "^0.12.7",
    "react-window": "^1.8.5"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
