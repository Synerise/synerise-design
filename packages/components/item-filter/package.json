{
  "name": "@synerise/ds-item-filter",
  "version": "0.8.96",
  "description": "ItemFilter UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.18.7",
    "@synerise/ds-drawer": "^0.6.4",
    "@synerise/ds-icon": "^0.58.7",
    "@synerise/ds-manageable-list": "^0.26.18",
    "@synerise/ds-result": "^0.6.37",
    "@synerise/ds-scrollbar": "^0.6.13",
    "@synerise/ds-search-bar": "^0.6.51",
    "@synerise/ds-tabs": "^0.13.86",
    "@synerise/ds-typography": "^0.13.3",
    "react-intl": "3.12.0",
    "react-window": "^1.8.5"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@synerise/ds-utils": "^0.24.21",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1"
  }
}
