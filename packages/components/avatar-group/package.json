{
  "name": "@synerise/ds-avatar-group",
  "version": "0.2.0",
  "description": "AvatarGroup UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-avatar": "^0.8.21",
    "@synerise/ds-badge": "^0.4.24",
    "@synerise/ds-button": "^0.12.19",
    "@synerise/ds-dropdown": "^0.11.57",
    "@synerise/ds-modal": "^0.11.40",
    "@synerise/ds-table": "^0.22.0",
    "@synerise/ds-tooltip": "^0.7.7"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "@synerise/ds-utils": "^0.8.28"
  }
}
