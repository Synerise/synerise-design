{
  "name": "@synerise/ds-avatar-group",
  "version": "0.6.99",
  "description": "AvatarGroup UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-avatar": "^0.15.4",
    "@synerise/ds-badge": "^0.6.36",
    "@synerise/ds-button": "^0.18.1",
    "@synerise/ds-dropdown": "^0.17.79",
    "@synerise/ds-icon": "^0.58.1",
    "@synerise/ds-modal": "^0.17.4",
    "@synerise/ds-table": "^0.46.33",
    "@synerise/ds-tooltip": "^0.14.3"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@synerise/ds-utils": "^0.24.15",
    "@testing-library/jest-dom": "5.1.1"
  }
}
