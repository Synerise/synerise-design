{
  "name": "@synerise/ds-subtle-form",
  "version": "0.4.2",
  "description": "SubtleForm UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-date-picker": "^0.2.20",
    "@synerise/ds-icon": "^0.34.0",
    "@synerise/ds-input": "^0.16.3",
    "@synerise/ds-select": "^0.12.49",
    "@synerise/ds-tooltip": "^0.9.3",
    "calculate-size": "^1.1.1"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "@synerise/ds-utils": "^0.11.7"
  }
}
