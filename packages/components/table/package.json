{
  "name": "@synerise/ds-table",
  "version": "0.46.13",
  "description": "Table UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^0.7.32",
    "@synerise/ds-button": "^0.17.16",
    "@synerise/ds-button-group": "^0.6.23",
    "@synerise/ds-checkbox": "^0.11.35",
    "@synerise/ds-column-manager": "^0.10.68",
    "@synerise/ds-data-format": "^0.3.4",
    "@synerise/ds-dropdown": "^0.17.65",
    "@synerise/ds-flag": "^0.4.0",
    "@synerise/ds-icon": "^0.56.0",
    "@synerise/ds-input": "^0.18.41",
    "@synerise/ds-loader": "^0.2.36",
    "@synerise/ds-menu": "^0.17.9",
    "@synerise/ds-pagination": "^0.7.23",
    "@synerise/ds-search": "^0.8.51",
    "@synerise/ds-search-bar": "^0.6.31",
    "@synerise/ds-select": "^0.15.6",
    "@synerise/ds-skeleton": "^0.2.35",
    "@synerise/ds-status": "^0.5.66",
    "@synerise/ds-tags": "^0.7.22",
    "@synerise/ds-tooltip": "^0.13.4",
    "@synerise/ds-typography": "^0.12.7",
    "@synerise/ds-utils": "^0.19.0",
    "@types/react-window": "^1.8.5",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "ramda": "0.27.0",
    "rc-resize-observer": "^0.2.1",
    "react-window": "^1.8.7",
    "unit-to-px": "^1.0.5"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "^4",
    "react": ">=16.9.0 < 17.0.0"
  }
}
