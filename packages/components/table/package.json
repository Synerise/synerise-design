{
  "name": "@synerise/ds-table",
  "version": "0.46.36",
  "description": "Table UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^0.7.44",
    "@synerise/ds-badge": "^0.6.38",
    "@synerise/ds-button": "^0.18.3",
    "@synerise/ds-button-group": "^0.6.31",
    "@synerise/ds-checkbox": "^0.11.45",
    "@synerise/ds-column-manager": "^0.11.12",
    "@synerise/ds-data-format": "^0.4.3",
    "@synerise/ds-dropdown": "^0.17.82",
    "@synerise/ds-flag": "^0.4.1",
    "@synerise/ds-icon": "^0.58.3",
    "@synerise/ds-input": "^0.19.3",
    "@synerise/ds-loader": "^0.2.45",
    "@synerise/ds-menu": "^0.18.0",
    "@synerise/ds-modal": "^0.17.6",
    "@synerise/ds-pagination": "^0.7.31",
    "@synerise/ds-scrollbar": "^0.6.8",
    "@synerise/ds-search": "^0.8.67",
    "@synerise/ds-select": "^0.15.21",
    "@synerise/ds-skeleton": "^0.3.10",
    "@synerise/ds-status": "^0.5.86",
    "@synerise/ds-tags": "^0.8.16",
    "@synerise/ds-tooltip": "^0.14.5",
    "@synerise/ds-typography": "^0.13.1",
    "@synerise/ds-utils": "^0.24.17",
    "@types/react-window": "^1.8.5",
    "classnames": "2.3.2",
    "copy-to-clipboard": "^3.3.1",
    "lodash": "^4.17.19",
    "ramda": "0.27.0",
    "rc-resize-observer": "^0.2.1",
    "react-intl": "3.12.0",
    "react-window": "^1.8.7",
    "unit-to-px": "^1.0.5",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.7.0",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "5.0.1"
  }
}
