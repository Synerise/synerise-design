{
  "name": "@synerise/ds-table",
  "version": "0.27.3",
  "description": "Table UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.13.12",
    "@synerise/ds-checkbox": "^0.9.20",
    "@synerise/ds-column-manager": "^0.7.67",
    "@synerise/ds-dropdown": "^0.11.97",
    "@synerise/ds-flag": "^0.2.8",
    "@synerise/ds-icon": "^0.34.0",
    "@synerise/ds-input": "^0.16.3",
    "@synerise/ds-menu": "^0.1.4",
    "@synerise/ds-pagination": "^0.6.41",
    "@synerise/ds-search": "^0.3.1",
    "@synerise/ds-search-bar": "^0.2.102",
    "@synerise/ds-select": "^0.12.49",
    "@synerise/ds-tags": "^0.2.131",
    "@synerise/ds-tooltip": "^0.9.3",
    "@synerise/ds-typography": "^0.9.5",
    "@synerise/ds-utils": "^0.11.7",
    "@types/react-window": "^1.8.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "rc-resize-observer": "^0.2.1",
    "react-window": "^1.8.5"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "^4",
    "react": ">=16.9.0 < 17.0.0"
  }
}
