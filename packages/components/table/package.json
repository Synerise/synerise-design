{
  "name": "@synerise/ds-table",
  "version": "0.60.11",
  "description": "Table UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --delete-dir-on-start --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "pack:ci": "npm pack --pack-destination ../../portal/storybook-static/static",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit",
    "upgrade:ds": "ncu -f \"@synerise/ds-*\" -u"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-alert": "^0.8.46",
    "@synerise/ds-badge": "^0.8.25",
    "@synerise/ds-button": "^0.22.3",
    "@synerise/ds-button-group": "^0.7.29",
    "@synerise/ds-checkbox": "^0.12.23",
    "@synerise/ds-column-manager": "^0.13.11",
    "@synerise/ds-data-format": "^0.6.1",
    "@synerise/ds-dropdown": "^0.18.31",
    "@synerise/ds-flag": "^0.5.3",
    "@synerise/ds-icon": "^0.70.0",
    "@synerise/ds-input": "^0.24.21",
    "@synerise/ds-loader": "^0.3.22",
    "@synerise/ds-menu": "^0.20.12",
    "@synerise/ds-modal": "^0.17.57",
    "@synerise/ds-pagination": "^0.7.78",
    "@synerise/ds-result": "^0.7.20",
    "@synerise/ds-scrollbar": "^0.11.24",
    "@synerise/ds-search": "^0.9.25",
    "@synerise/ds-select": "^0.16.33",
    "@synerise/ds-skeleton": "^0.6.24",
    "@synerise/ds-status": "^0.7.4",
    "@synerise/ds-tags": "^0.10.25",
    "@synerise/ds-tooltip": "^0.14.55",
    "@synerise/ds-typography": "^0.16.12",
    "@synerise/ds-utils": "^0.31.2",
    "@types/react-window": "^1.8.5",
    "classnames": "2.3.2",
    "copy-to-clipboard": "^3.3.1",
    "lodash": "^4.17.21",
    "ramda": "0.27.0",
    "rc-resize-observer": "^1.2.0",
    "rc-table": "~7.26.0",
    "react-window": "^1.8.7",
    "unit-to-px": "^1.0.5",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "antd": "4.24.16",
    "react": ">=16.9.0 <= 18.3.1",
    "react-intl": ">=3.12.0 <= 6.8",
    "styled-components": "^5.3.3"
  }
}
