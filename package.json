{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Synerise/synerise-design.git"
  },
  "workspaces": [
    "packages/components/*",
    "packages/storybook"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "yarn build:core && yarn build:all",
    "build:all": "lerna run build",
    "build:core": "lerna run build --scope=@synerise/ds-core",
    "build-storybook": "cd packages/storybook && yarn build-storybook",
    "clean": "lerna clean --yes && rm -rf node_modules && rm -rf packages/components/*/dist",
    "component:create": "node ./scripts/create-component/index.js",
    "lerna:version": "lerna version --conventional-commits",
    "lint": "eslint ./packages/components --ext ts,tsx",
    "lint:fix": "eslint ./packages/components --fix --ext ts,tsx",
    "prettier": "prettier --write ./packages/components",
    "pack:ci": "lerna run pack:ci --since origin/master",
    "storybook": "cd packages/storybook && yarn storybook",
    "test:ci": "lerna run test --stream --concurrency 3",
    "tgz-package-json": "node ./scripts/tgz/index.cjs",
    "types": "lerna run types --stream  --concurrency 5",
    "check:circular-dependencies": "lerna run check:circular-dependencies",
    "upgrade:ds": "lerna run upgrade:ds",
    "knip": "knip"

  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "@jest/fake-timers": "^27",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "babel-jest": "^27",
    "jest": "^27",
    "prettier": "^3.5.3",
    "rc-trigger": "^5.3.4",
    "rc-util": "^5.43.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "volta": {
    "node": "22.14.0",
    "yarn": "1.22.22"
  },
  "dependencies": {
    "antd": "4.24.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-perfect-scrollbar": "^1.5.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@commitlint/config-workspace-scopes": "^19.8.1",
    "@formatjs/intl-pluralrules": "5.3.1",
    "@formatjs/intl-relativetimeformat": "11.4.1",
    "@fortawesome/fontawesome-svg-core": "^6",
    "@fortawesome/free-brands-svg-icons": "^6",
    "@fortawesome/free-regular-svg-icons": "^6",
    "@fortawesome/free-solid-svg-icons": "^6",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@jest/fake-timers": "^27",
    "@jridgewell/gen-mapping": "0.3.2",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.11.22",
    "@swc/jest": "^0.2.38",
    "@synerise/eslint-config": "^0.1.2",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "^10.3.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/debounce": "^1.2.1",
    "@types/flat": "5.0.3",
    "@types/highlightjs": "^9.12.6",
    "@types/jest": "27",
    "@types/node": "^24.9.2",
    "@types/ramda": "^0.28.23",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-onclickoutside": "^6.7.4",
    "@types/react-window": "^1.8.8",
    "@types/resize-observer-browser": "^0.1.7",
    "@types/styled-components": "5.1.27",
    "@types/uuid": "^8.0.0",
    "@welldone-software/why-did-you-render": "^6.0.3",
    "babel-jest": "27",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "copy-template-dir": "1.4.0",
    "css-loader": "^7.1.2",
    "format-package": "5.2.0",
    "husky": "4.2.3",
    "inquirer": "7.1.0",
    "jest": "^27",
    "jest-environment-jsdom": "^27",
    "knip": "^5.66.4",
    "lerna": "^8.2.2",
    "less": "3.11.1",
    "less-loader": "^12.3.0",
    "less-plugin-clean-css": "^1.6.0",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": "16.1.0",
    "madge": "^8.0.0",
    "npm-check-updates": "^16",
    "react-intl": "^6.8.7",
    "sass": "^1.43.4",
    "style-loader": "^4.0.0",
    "styled-components": "^5.3.3",
    "stylelint": "16.20.0",
    "stylelint-config-standard": "38.0.0",
    "typescript": "^5.9.3",
    "webpack": "^5.98.0"
  }
}